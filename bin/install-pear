#!/usr/bin/env expect

set send_slow {1 .1}
set timeout -1

spawn ./bin/php go-pear.phar

expect -re "1-\d+, 'all' or Enter to continue:"
sleep .1
send -s -- "\r"

expect -re "Would you like to alter php.ini"
sleep .1
send -s -- "n\r"

expect "Press Enter to continue:"
sleep .1
send -s -- "\r"
expect EOF